library(ggplot2)
# create sample data
data <- data.frame(
group = c("Group A", "Group B"),
value = c(5, 8),
sd = c(1, 2)
)
# calculate standard error of the mean
data$sem <- data$sd / sqrt(nrow(data))
# calculate confidence intervals for the mean (95% CI)
data$ci_upper <- data$value + qt(0.975, df = nrow(data) - 1) * data$sem
data$ci_lower <- data$value - qt(0.975, df = nrow(data) - 1) * data$sem
# create ggplot object
ggplot(data, aes(x = group, y = value)) +
# add bars with error bars
geom_bar(stat = "identity", fill = "steelblue", width = 0.5) +
geom_errorbar(aes(ymin = ci_lower, ymax = ci_upper), width = 0.2) +
# customize x and y axis labels and limits
xlab("Group") + ylab("Value") +
ylim(0, max(data$value) * 1.2) +
# add asterisks to indicate significant differences
annotate("text", x = 1, y = data$value[1] + data$sem[1] + 0.5, label = "***", size = 6) +
# customize plot theme
theme_classic() +
theme(plot.title = element_text(hjust = 0.5),
axis.title = element_text(size = 12, face = "bold"),
axis.text = element_text(size = 10),
axis.line = element_line(size = 0.5),
panel.grid.major = element_blank(),
panel.grid.minor = element_blank(),
